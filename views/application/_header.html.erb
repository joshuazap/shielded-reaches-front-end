<!-- Static navbar -->
<nav class="navbar navbar-default navbar-fixed-top navbar-transparent">
  <div class="navbar-header">
    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
    <a class="navbar-brand" href="https://www.cardinalhire.com">
      <span style="display: inline-block; vertical-align: top;">
        <%= image_tag('Asset 2@svg.svg', alt: 'CardinalHire', class: 'logo-default', width: '30', height: '30') %>
        <%= image_tag('Asset 1@svg.svg', alt: 'CardinalHire', class: 'logo-scroll', width: '30', height: '30') %>
        <%= content_tag(:p, "CardinalHire", class: "logo-text-default text-bold") %>
        <%= content_tag(:p, "CardinalHire", class: "logo-text-scroll text-bold") %>
      </span>
    </a>
  </div>
  <div id="navbar" class="navbar-collapse collapse">
    <div id="custom_navbar">
      <% if current_user && current_user.signuprole == 'talent' %>
        <%= render 'job_seeker_navbar' %>
      <% elsif current_user && current_user.signuprole == 'employer' %>
        <%= render 'employer_navbar' %>
      <% elsif current_user && current_user.role == 'admin' %>
        <%= render 'admin_navbar' %>
      <% elsif current_user && current_user.role == 'member' %>
        <%= render 'member_navbar' %>
      <% elsif current_user && current_user.role == 'recruiter' %>
        <%= render 'recruiter_navbar' %>
      <% else %>
        <%= render 'guest_navbar' %>
      <% end %>
    </div>
  </div>
</nav><!--static nav-collapse end-->

<!--pre-loader-->
<div id="preloader" style="background: #fff url('<%= image_url("loader.gif") %>') no-repeat center center;"></div>
<!--pre-loader end-->

<!--back to top-->
<a href="#" class="scrollToTop">
  <i class="fa fa-arrow-circle-up" style="color: #fff;"></i>
</a>
<!--back to top end-->
