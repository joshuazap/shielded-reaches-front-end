<div class="nested-fields">
  <%= f.input :company_name %>
  <%= f.input :title %>
  <%= f.input :start_date, :as => :date, html5: true %>
  <%= f.input :end_date, :as => :date, html5: true %>
  <%= f.input :description %>
  <%= link_to_remove_association f, class: "btn btn-default btn-xs" do %>
    <i class="fa fa-trash"></i>
  <% end %>
</div>
