    <div class="divimage dzsparallaxer--target " data-src="<%= image_url('bg4.jpg') %>" style="width: 100%; height: 100%; background-image: url('<%= image_url('bg4.jpg') %>'); background-size: cover;">
        <div class="hero-parallax">
            <div class="hero-inner">
                <div class="hero-content">
                    <div class="container text-center">
                        <div class="row">
                            <div class="col-sm-10 col-sm-offset-1">
